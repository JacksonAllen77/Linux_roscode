<launch>
    <!-- 运行地图服务器，并且加载设置的地图-->
    <include file="$(find slam_test)/launch/read_map.launch" />
    <!-- 启动AMCL节点 -->
    <include file="$(find slam_test)/launch/amcl.launch" />
    <!-- 运行move_base节点 -->
    <include file="$(find slam_test)/launch/move_base.launch" />
    <!-- 运行rviz -->
    <node pkg="joint_state_publisher" name="joint_state_publisher" type="joint_state_publisher" />
    <node pkg="robot_state_publisher" name="robot_state_publisher" type="robot_state_publisher" />
    <node pkg="rviz" type="rviz" name="rviz"/>

</launch>