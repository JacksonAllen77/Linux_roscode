<launch>
    <!-- clear_params为true表示程序启动后,会将所有的参数进行清除；respawn为false表示节点关闭后不会被重启 -->
    <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen" clear_params="true">
        <rosparam file="$(find slam_test)/param/costmap_common_params.yaml" command="load" ns="global_costmap" />
        <rosparam file="$(find slam_test)/param/costmap_common_params.yaml" command="load" ns="local_costmap" />
        <rosparam file="$(find slam_test)/param/local_costmap_params.yaml" command="load" />
        <rosparam file="$(find slam_test)/param/global_costmap_params.yaml" command="load" />
        <rosparam file="$(find slam_test)/param/base_local_planner_params.yaml" command="load" />
    </node>

</launch>